{
  "name": "open-project",
  "displayName": "Open Project",
  "version": "1.1.0",
  "description": "Quickly open projects in VS Code",
  "categories": [
    "Other"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mskelton/vscode-open-project"
  },
  "publisher": "mskelton",
  "files": [
    "LICENSE",
    "out/**/*",
    "resources/**/*"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint .",
    "format": "oxfmt .",
    "format:check": "oxfmt --check ."
  },
  "devDependencies": {
    "@mskelton/eslint-config": "^9.2.0",
    "@types/node": "^22.15.17",
    "@types/vscode": "^1.60.0",
    "eslint": "^9.29.0",
    "oxfmt": "^0.24.0",
    "typescript": "^5.8.3"
  },
  "activationEvents": [
    "onCommand:open-project.open",
    "onCommand:open-project.open-in-new-window"
  ],
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.60.0"
  }
}
